- 
  const sponsors = [
    {
      name: "株式会社サイバーエージェント AI Lab",
      link: "https://cyberagent.ai/ailab/",
      logo: "caailab",
    },
    {
      name: "ほぼ日 サイエンス・マジック部",
      link: "https://www.1101.com/n/s/boshu_sm",
      logo: "hobonichi_sm",
    },
  ]

h2 企業スポンサー（五十音順）
.row
  each sponsor in sponsors
    - const image_path = "./assets/sponsors/" + sponsor["logo"] + ".png"
    .col-lg-4.col-md-6.col-12.text-center
      a(href=sponsor["link"])
        img.img-fluid(src=image_path alt=sponsor["name"])
        p #{sponsor["name"]}
